layer {
  name : "FC_01"
  type : "InnerProduct"
  bottom : "conv05"
  top : "fc01"
  inner_product_param{
    num_output: 150
    weight_filler {type: "gaussian" std:0.01}
    bias_filler {type: "constant"}
  }
}
