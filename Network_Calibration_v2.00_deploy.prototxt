layer {
  name: "Conv_01"
  type: "Convolution"
  bottom: "in_data"
  top: "conv01"
  param { lr_mult: 1 }
  param { lr_mult: 2 }
  convolution_param {
    num_output: 32
    kernel_size: 5
    pad: 0
    stride: 2
    weight_filler { type: "xavier" }
    bias_filler { type: "constant" }
  }
}
layer {
  name: "ReLU_01"
  type: "ReLU"
  bottom: "conv01"
  top: "conv01"
}

layer {
  name: "Conv_02"
  type: "Convolution"
  bottom: "conv01"
  top: "conv02"
  param { lr_mult: 1 }
  param { lr_mult: 2 }
  convolution_param {
    num_output: 32
    kernel_size: 5
    pad : 0
    stride: 2
    weight_filler { type: "xavier" }
    bias_filler { type: "constant" }
  }
}
layer {
  name: "ReLU_02"
  type: "ReLU"
  bottom: "conv02"
  top: "conv02"
}

layer {
  name: "Conv_03"
  type: "Convolution"
  bottom: "conv02"
  top: "conv03"
  param { lr_mult: 1 }
  param { lr_mult: 2 }
  convolution_param {
    num_output: 32
    kernel_size: 5
    pad: 0
    stride: 2
    weight_filler { type: "xavier" }
    bias_filler { type: "constant" }
  }
}
layer {
  name: "ReLU_03"
  type: "ReLU"
  bottom: "conv03"
  top: "conv03"
}

layer {
  name: "Conv_04"
  type: "Convolution"
  bottom: "conv03"
  top: "conv04"
  param { lr_mult: 1 }
  param { lr_mult: 2 }
  convolution_param {
    num_output: 32
    kernel_size: 5
    pad : 0
    stride: 2
    weight_filler { type: "xavier" }
    bias_filler { type: "constant" }
  }
}
layer {
  name: "ReLU_04"
  type: "ReLU"
  bottom: "conv04"
  top: "conv04"
}

layer {
  name: "Conv_05"
  type: "Convolution"
  bottom: "conv04"
  top: "conv05"
  param { lr_mult: 1 }
  param { lr_mult: 2 }
  convolution_param {
    num_output: 32
    kernel_size: 5
    pad: 0
    stride: 2
    weight_filler { type: "xavier" }
    bias_filler { type: "constant" }
  }
}

layer {
  name: "ReLU_05"
  type: "ReLU"
  bottom: "conv05"
  top: "conv05"
}

